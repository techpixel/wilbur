generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Participant {
  participantId     Int     @id @default(autoincrement())
  slackId           String       
  ysws              String

  acceptedIntegrity Boolean  @default(false)
  hackatimeProject  String?
  gitLink           String?
  journalLink       String?
  submitted         Boolean  @default(false)
}